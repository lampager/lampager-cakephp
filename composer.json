{
    "name": "lampager/lampager-cakephp",
    "description": "Rapid pagination for CakePHP 5",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Chitoku",
            "email": "odango@chitoku.jp"
        }
    ],
    "keywords": ["cakephp", "pagination", "paginator", "offset", "limit"],
    "autoload": {
        "psr-4": {
            "Lampager\\Cake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lampager\\Cake\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "cakephp/cakephp": "^5.1",
        "lampager/lampager": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1 || ^11.0",
        "nilportugues/sql-query-formatter": "^1.2"
    },
    "scripts": {
        "test": "phpunit"
    }
}
